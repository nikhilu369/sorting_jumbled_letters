s=z.lower()
print('The letter u entered')
print([x for x in s])
le=len(s)

count= len(s)+1

print('All of possible combination:')
for ti in range(1,len(s)):
 count=count-1
 for ye in permutations(s,count):
  c=(''.join(ye))
  mw.add(c) 

